<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    
	<string-array name="wifi_connect_type_entries">
        <!-- auto connect to wifi -->
        <item>Automatically connect</item>
        <!-- manual connect to wifi -->
        <item>Manually connect</item>
        <!-- always notify user when wifi is available -->
        <item>Always ask</item>
    </string-array>
	<string-array name="wifi_connect_type_values">
        <!-- auto connect to wifi -->
        <item>0</item>
        <!-- manual connect to wifi -->
        <item>1</item>
        <!-- always notify user when wifi is available -->
        <item>2</item>
    </string-array>

	<!-- Camera Preferences White Balance for CMCC menu entries -->
	<string-array name="pref_camera_whitebalance_entries_cmcc" translatable="false">
	    <item>@string/pref_camera_whitebalance_entry_auto</item>
	    <item>@string/pref_camera_whitebalance_entry_incandescent</item>
	    <item>@string/pref_camera_whitebalance_entry_daylight</item>
	    <item>@string/pref_camera_whitebalance_entry_fluorescent</item>
	    <item>@string/pref_camera_whitebalance_entry_cloudy</item>
	    <item>@string/pref_camera_whitebalance_entry_twilight</item>
	    <item>@string/pref_camera_whitebalance_entry_shade</item>
	    <item>@string/pref_camera_whitebalance_entry_warm_fluorescent</item>
	    <item>@string/pref_camera_whitebalance_entry_tungsten</item>
	</string-array>
	
	<!-- apn type for cmcc -->
	<string-array name="apn_type_cmcc" translatable="false">
        <item>default</item>
        <item>mms</item>
        <item>supl</item>
        <item>wap</item>
        <item>net</item>
        <item>cmmail</item>
    </string-array>
  <!--Agps settings-->
  <string-array name ="network_use_entry">
    	<item>Only Home</item>
    	<item>Home + Roaming</item>
    </string-array>

</resources>
